@using LtInfo.Common.AgGridWrappers
@using ProjectFirma.Web.Models
@using ProjectFirma.Web.Views
@using ProjectFirma.Web.Views.Shared
@inherits ProjectFirma.Web.Views.InteractionEvent.InteractionEventIndex


@section JavascriptAndStylesContent
{
    @{ AgGridIncludes.RenderPartialView(Html); }
    <script src="@Url.Content("~/Views/InteractionEvent/InteractionEventController.js")" type="text/javascript"></script>

}


<div style="padding-top: 10px;">
    @{ ViewPageContent.RenderPartialView(Html, ViewDataTyped.ViewPageContentViewData); }
</div>

<div style="margin-top: 10px">

    <h3>@FieldDefinition.InteractionEvent.GetFieldDefinitionLabelPluralized()</h3>
    @Html.AgGrid(ViewDataTyped.InteractionEventGridSpec, ViewDataTyped.InteractionEventGridName, ViewDataTyped.InteractionEventGridDataUrl, "height: 255px", AgGridResizeType.VerticalResizableHorizontalAutoFit)


</div>

